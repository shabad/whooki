{
    "manifest_version": 2,

    "name": "Click It",
    "description": "My first Chrome extension.",
    "version": "1.0",
    "content_scripts": [{
      "js": ["jquery.js"],
      "matches": ["http://*/*", "https://*/*"]
    }],
    "author": "Emad Elmogy",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "My custom google Extension"
    },

    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "https://*/"
     ]
}
