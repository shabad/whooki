{
    "manifest_version": 2,

    "name": "Whooki",
    "description": "Community for websites",
    "version": "1.0",
    "author": "Shabad Sobti",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "My custom google Extension"
    },

    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "http://localhost:8000/",
        "http://localhost:8000/users/login",
        "*://*/"
     ]
}
